[Argument]
CaptureAccountToken = switch, true, false, tag = 捕获账号令牌, desc = 此开关控制插件是否捕获账户令牌
cron = input,"0 8 * * *",tag=定时任务时间,desc=定时任务的cron表达式
appName = input,"emby",tag=应用名称,desc=填写app的用户名，用于提交app的相关信息
appCategory = select,"Tool","Video&Photo","Game",tag=应用类型



[Script]
http-request ^https:\/\/.+\/emby\/Sessions\/Playing\/Stopped script-path=https://raw.githubusercontent.com/ZenmoFeiShi/Qx/main/Emby.js, requires-body=true, timeout=60, tag=Emby,argument=[{appName},{appCategory}], enable={CaptureAccountToken}

cron｛cron｝script-path=https://raw.githubusercontent.com/ZenmoFeiShi/Qx/main/Emby.js, timeout=120, tag=Emby自动观看, img-url=https://raw.githubusercontent.com/fmz200/wool_scripts/main/icons/lige47/emby.png, enable={CaptureAccountToken}
[MITM]

hostname = -*.fileball.app, *


